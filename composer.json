{
  "name": "sanderdewijs/lara-livewire-maps",
  "description": "Laravel Livewire 3 Google Maps component with markers, events, and optional clustering.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "livewire",
    "livewire3",
    "google-maps",
    "maps",
    "component"
  ],
  "require": {
    "php": "^8.2",
    "livewire/livewire": "^3.6",
    "illuminate/support": "^10.0 || ^11.0",
    "illuminate/contracts": "^10.0 || ^11.0"
  },
  "require-dev": {
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Sdw\\LivewireMaps\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sdw\\LivewireMaps\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sdw\\LivewireMaps\\LivewireMapServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "pest",
    "pest": "pest",
    "serve": [
      "Composer\\Config::disableProcessTimeout"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
